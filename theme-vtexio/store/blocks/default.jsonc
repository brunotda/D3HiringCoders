{
  "store.orderplaced": {
    "blocks": [
      "order-placed"
    ]
  },
  "store.account": {
    "blocks": [
      "my-account"
    ],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  "store.login": {
    "blocks": [
      "login-content#default"
    ]
  },
  "login-content#default": {
    "props": {
      "isInitialScreenOptionOnly": false,
      "defaultOption": 1,
      "showPasswordVerificationIntoTooltip": true
    }
  },
  "add-to-cart-button#shelf":{
    "props":{
      "text": "Ver Detalhes"
    }
  },
  "rich-text#loading": {
    "props": {
      "text": "Carregando..."
    }
  },
  "product-price-suspense": {
    "props": {
      "Fallback": "rich-text#loading"
    },
    "children": [
      "flex-layout.row#priceSelling"
    ]
  },
  "flex-layout.row#priceSelling":{
    "props":{
      "horizontalAlign": "center",
      "colSizing": "equal",
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children":[
      "product-selling-price",
      "add-to-cart-button#shelf"
    ]
  },
  "product-summary.shelf": {
    "children": [
      "product-summary-name",
      "product-summary-image",
      "product-summary-add-to-list-button",
      "product-rating-inline",
      "product-summary-space",
      "product-price-suspense"
    ]
  },
  "product-summary-image": {
    "props": {
      "showBadge": false
    }
  },
  "rich-text":{
    "props": { 
      "text": "**Apenas um texto, este recurso n√£o esta configurado.**",
      "textPosition": "CENTER"
    }
  }
}